{
  "name": "doc-management",
  "version": "1.0.0",
  "description": "A full stack document management system",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./bin/www --exec babel-node",
    "build": "babel bin -d dist ",
    "serve": "node dist/www.js",
    "test": "NODE_ENV=test npm run db:test:seed && npm run thetest",
    "thetest": "NODE_ENV=test mocha ./server/test/**/*.spec.js --compilers js:babel-register",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "babel-node ./server/test/helper/seed-helper"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-osubair/doc-management.git"
  },
  "author": "Subair O.A",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-osubair/doc-management/issues"
  },
  "homepage": "https://github.com/andela-osubair/doc-management#readme",
  "dependencies": {
    "body-parser": "^1.17.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^4.0.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "morgan": "^1.8.1",
    "pg": "^6.1.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.30.2",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}

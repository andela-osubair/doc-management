{
  "name": "doc-management",
  "version": "1.0.0",
  "description": "A full stack document management system",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node bin/startMessage.js",
    "heroku-postbuild": "npm run build",
    "start": "npm-run-all --parallel open:www",
    "open:www": "babel-node bin/www.js",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "lint:watch": "npm run lint -- --watch",
    "test:e2e": "nightwatch --config nightwatch.conf.js",
    "test": "NODE_ENV=test npm run db:test:seed && npm run cover",
    "mocha:test": "NODE_ENV=test npm run db:test:seed && mocha --reporter spec ./server/test/testSetup.js \"./server/test/**/*.spec.js\" --timeout 60000",
    "cover": "NODE_ENV=test nyc --reporter=html --reporter=text _mocha --reporter spec ./server/test/testSetup.js \"./server/test/**/*.spec.js\" --timeout=60000",
    "test:watch": "npm run mocha:test -- --watch",
    "test:all": "npm run db:test:seed && npm run thetest:watch",
    "db:migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate:test": "NODE_ENV=test sequelize db:migrate",
    "db:test:seed": "NODE_ENV=test babel-node ./server/test/helper/seed-helper",
    "db:seed": "babel-node ./server/test/helper/seed-helper",
    "coveralls": "nyc report --reporter=text-lcov |  coveralls",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node bin/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build" : "babel-node bin/build.js",
    "postbuild": "babel-node bin/distServer.js",
    "bundled":"babel bin/ --out-dir dist/bin --ignore bin/build*.js,bin/www.js,bin/startMessage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-osubair/doc-management.git"
  },
  "author": "Subair O.A",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andela-osubair/doc-management/issues"
  },
  "homepage": "https://github.com/andela-osubair/doc-management#readme",
